# Copy to .env and fill in. Do not commit .env.

# Site name (meta title, og:title)
VITE_APP_NAME=Tiny Vivid Minds

# Meta description and og:description
VITE_APP_DESCRIPTION=Your app tagline here

# Meta author
VITE_APP_AUTHOR=Your org name

# Full URL for og:image and twitter:image, or /og.png (add image to public/og.png)
VITE_OG_IMAGE=https://yoursite.com/og.png

# Twitter handle (e.g. @tinyvividminds)
VITE_TWITTER_SITE=@tinyvividminds

# Contact phone for display and tel: link (e.g. +919876543210)
VITE_CONTACT_PHONE=+1234567890

# Contact email for display and mailto:
VITE_CONTACT_EMAIL=info@example.com

# WhatsApp number for wa.me link: country code + number, no + (e.g. 919876543210)
VITE_WHATSAPP_NUMBER=1234567890
